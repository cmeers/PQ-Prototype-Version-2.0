<!DOCTYPE html>
<html>
<head>
  <title>PrettyQuick</title>
  <%= render 'layouts/head' %> 
</head>
<body>
  
  <style type="text/css">
        body {
          padding-top: 60px;
        }
  </style>
<div class="topbar">
  <div class="topbar-inner">
    <div class="container-fluid">
      <%= link_to 'Pretty Quick', root_url, :class => 'brand'  %>
      <ul class="nav">
        <%= render 'layouts/navigation' %>
      </ul>

      <p class="pull-right">
        <%= render 'layouts/current_user' %>
      </p>
      
    </div>   
  </div>
</div>      

<div class="container-fluid">

  <div class="sidebar">
    <%= render 'layouts/sidebar' %>
  </div>
                        
    <div class="content">
      <%= yield %>
       <%# session[:latlng] %></span>
      <!-- <p>Session:<%= session.inspect %></p> -->
    </div>
  
</div>

<%= yield :scripts %>
</body>
</html>
